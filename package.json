{
  "name": "ngx-json-table",
  "version": "2.1.3",
  "description": "Show and edit a JSON object using HTML table.",
  "repository": {
    "url": "https://github.com/bacali95/ngx-json-table.git",
    "type": "git"
  },
  "author": {
    "name": "Nasreddine Bac Ali",
    "email": "nasreddine.bacali95@gmail.com",
    "url": "https://nasreddinebacali.info"
  },
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:demo": "ng build demo --configuration production",
    "build:lib": "ng build ngx-json-table --configuration production",
    "build:ci": "yarn build:demo && yarn build:lib",
    "deploy:demo": "yarn build:demo -- --base-href '/ngx-json-table/' && npx angular-cli-ghpages --dir=./dist/demo",
    "test": "ng test ngx-json-table",
    "release": "yarn build:lib && yarn publish",
    "publish": "npm publish --access=public dist/ngx-json-table",
    "changelog": "standard-changelog -p angular",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"**/*.{ts,js,json,html,scss,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,html,scss,md}\"",
    "commit": "git-cz",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:auto": "standard-version",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "bootstrap": "^5.3.2",
    "font-awesome": "^4.7.0",
    "highlight.js": "^11.11.1",
    "jquery": "^3.7.1",
    "material-design-icons": "^3.0.1",
    "minimist": "^1.2.8",
    "ngx-highlightjs": "^12.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@angular/language-service": "^17.0.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@types/jasmine": "~5.1.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "angular-cli-ghpages": "^2.0.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^17.0.0",
    "prettier": "^3.5.2",
    "raw-loader": "^4.0.2",
    "standard-changelog": "^5.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.2.2",
    "webpack": "^5.98.0"
  },
  "volta": {
    "node": "22.14.0",
    "yarn": "4.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
